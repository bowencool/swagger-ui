{
  "presets": [
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions", "not ie <= 8"]
        }
      }
    ]
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-vue-jsx",
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    ["import", { "libraryName": "ant-design-vue", "libraryDirectory": "es" }]
  ]
}
